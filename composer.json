{
  "name": "contributte/framex",
  "description": "Bootloader for Framework X and Nette Framework (@nettefw).",
  "keywords": [
    "nette",
    "frameworkx",
    "bootloader",
    "engine",
    "async"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/framex",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "clue/framework-x": "^0.16.0",
    "psr/container": "^2.0.2",
    "nette/utils": "^4.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4.0",
    "contributte/phpstan": "^0.1.0",
    "contributte/tester": "^0.3.0",
    "mockery/mockery": "^1.5.1",
    "nette/di": "^3.1.0",
    "tracy/tracy": "^2.9.5"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\FrameX\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
